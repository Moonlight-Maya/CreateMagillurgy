{
  "schemaVersion": 1,
  "id": "create-magillurgy",
  "version": "${version}",
  "name": "Create: Magillurgy",
  "description": "An addon for the Create mod that adds a new magic-based (yet still ultimately scientific and automatable) system to the game, unlocking new machines, recipes, and factories to create!",
  "authors": [
    "Limit System"
  ],
  "contact": {
    "sources": "https://github.com/Moonlight-Maya/CreateMagillurgy"
  },
  "license": "MIT",
  "icon": "assets/create-magillurgy/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.moonlight_maya.create_magillurgy.MagillurgyAddon"
    ]
  },
  "mixins": [
    "create-magillurgy.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}",
    "minecraft": "${minecraft_version}"
  }
}
